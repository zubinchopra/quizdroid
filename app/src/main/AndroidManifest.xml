<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.washington.zubinc.quizdroid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BeginMessage" />
        <activity android:name=".MathQuiz" />
        <activity
            android:name=".PhysicsQuiz"
            android:label="@string/title_activity_physics_quiz"
            android:parentActivityName=".BeginMessage">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.washington.zubinc.quizdroid.BeginMessage" />
        </activity>
        <activity
            android:name=".MarvelQuiz"
            android:label="@string/title_activity_marvel_quiz"
            android:parentActivityName=".BeginMessage">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.washington.zubinc.quizdroid.BeginMessage" />
        </activity>
        <activity android:name=".CheckAnswer" />
        <activity android:name=".MathQuiz2" />
        <activity
            android:name=".PhysicsQuiz2"
            android:label="@string/title_activity_physics_quiz2" />
        <activity
            android:name=".MarvelQuiz2"
            android:label="@string/title_activity_marvel_quiz2"></activity>
    </application>

</manifest>